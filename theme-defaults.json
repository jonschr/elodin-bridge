{
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "spacing": {
      "spacingSizes": [
        {
          "name": "Extra Extra Small",
          "size": "clamp(.25rem, 1.25vw, .5rem)",
          "slug": "xx-small"
        },
        {
          "name": "Extra Small",
          "size": "clamp(.375rem, 1.875vw, .75rem)",
          "slug": "x-small"
        },
        {
          "name": "Small",
          "size": "clamp(.5rem, 2.5vw, 1rem)",
          "slug": "small"
        },
        {
          "name": "Base",
          "size": "clamp(1rem, 3.25vw, 1.5rem)",
          "slug": "base"
        },
        {
          "name": "Medium",
          "size": "clamp(1.5rem, 4vw, 2rem)",
          "slug": "medium"
        },
        {
          "name": "Large",
          "size": "clamp(2rem, 5vw, 3rem)",
          "slug": "large"
        },
        {
          "name": "Extra Large",
          "size": "clamp(3rem, 7vw, 5rem)",
          "slug": "x-large"
        },
        {
          "name": "2xl",
          "size": "clamp(4rem, 9vw, 7rem)",
          "slug": "xx-large"
        },
        {
          "name": "3xl",
          "size": "clamp(5rem, 12vw, 9rem)",
          "slug": "xxx-large"
        },
        {
          "name": "4xl",
          "size": "clamp(6rem, 14vw, 13rem)",
          "slug": "xxxx-large"
        }
      ],
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ],
      "blockGap": true,
      "padding": true,
      "margin": true
    },
    "typography": {
      "dropCap": false,
      "customFontSize": true,
      "defaultFontSizes": false,
      "fontWeight": true,
      "letterSpacing": false,
      "fontStyle": false,
      "lineHeight": true,
      "textDecoration": false,
      "fontSizes": [
        {
          "slug": "x-small",
          "size": "clamp(.825rem, 0.825rem + ((1vw - 0.2rem) * 0.189), .95rem)",
          "name": "X Small"
        },
        {
          "slug": "small",
          "size": "clamp(.9rem, 0.9rem + ((1vw - 0.2rem) * 0.226), 1.05rem)",
          "name": "Small"
        },
        {
          "slug": "base",
          "size": "clamp(1rem, 1rem + ((1vw - 0.2rem) * 0.249), 1.165rem)",
          "name": "Base"
        },
        {
          "slug": "medium",
          "size": "clamp(1.2rem, 1.2rem + ((1vw - 0.2rem) * 0.679), 1.65rem)",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "clamp(1.5rem, 1.5rem + ((1vw - 0.2rem) * 1.887), 2.75rem)",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "clamp(1.875rem, 1.875rem + ((1vw - 0.2rem) * 2.453), 3.5rem)",
          "name": "X Large"
        },
        {
          "slug": "xx-large",
          "size": "clamp(2.25rem, 2.25rem + ((1vw - 0.2rem) * 3.227), 4.3875rem)",
          "name": "XX Large"
        }
      ]
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": false
        }
      },
      "core/separator": {
        "color": {
          "background": true
        }
      },
      "core/pullquote": {
        "border": {
          "color": true,
          "radius": false,
          "style": false,
          "width": false
        }
      }
    }
  },
  "styles": {
    "typography": {
      "lineHeight": "1.6"
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "5px",
          "style": "solid",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "top": "var(--space-s)",
            "right": "var(--space-m)",
            "bottom": "var(--space-s)",
            "left": "var(--space-m)"
          }
        },
        "typography": {
          "fontSize": "var(--font-s)",
          "fontWeight": "800",
          "letterSpacing": "0"
        },
        "variations": {
          "outline": {
            "border": {
              "width": "1px"
            }
          }
        }
      },
      "core/separator": {
        "spacing": {
          "margin": {
            "top": "var(--space-m)",
            "bottom": "var(--space-m)"
          }
        },
        "border": {
          "color": "var(--border-base)",
          "style": "solid",
          "width": "1px"
        }
      },
      "core/paragraph": {
        "typography": {
          "lineHeight": "1.6"
        }
      }
    }
  }
}
